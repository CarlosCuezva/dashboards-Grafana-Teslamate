          "rawQuery": true,
          "rawSql": "WITH c AS (\n\tSELECT *, COALESCE(LAG(start_date) OVER(ORDER BY start_date DESC), now()) AS next_start_date \n\tFROM charging_processes\n\tWHERE car_id = $car_id\n),\nrng AS (\n\tSELECT\n\t\tdate,\n\t\tdistance,\n\t\tduration_min,\n\t \tmax_battery_level,\n\t\tmin_battery_level\n\tFROM (\n\t\tSELECT d.start_date AS date, d.distance, d.duration_min, MAX(p.battery_level) AS max_battery_level, MIN(p.battery_level) AS min_battery_level\n\t\tFROM drives d\n\t\tLEFT JOIN positions p ON p.drive_id = d.id\n\t\tWHERE \n\t\t\td.car_id = $car_id\n\t\t\tAND $__timeFilter(d.start_date)\n\t\tGROUP BY 1, 2, 3\n\t\tHAVING COUNT(p.id) > 0\n\t) AS data\n\tGROUP BY 1, 2, 3, 4, 5\n),\nfnd AS (\n\tSELECT\n\t\tc.end_date,\n\t\tc.next_start_date,\n\t\tCOALESCE(SUM(r.distance), 0) AS distance_driven,\n\t\tCOALESCE(SUM(r.duration_min), 0) AS duration_min,\n\t\tr.max_battery_level,\n\t\tr.min_battery_level\n\tFROM c c\n\tLEFT JOIN rng r ON r.date BETWEEN c.start_date and c.next_start_date\n\tGROUP BY 1, 2, 5, 6\n)\nSELECT \n\t(round(extract(epoch FROM end_date) - 10) * 1000) AS start_date_ts,\n\t(round(extract(epoch FROM next_start_date) + 10) * 1000) AS end_date_ts,\n\tend_date, next_start_date,\n\tCASE WHEN (duration_min / 60) > 0\n\t\tTHEN convert_km((distance_driven / (duration_min::numeric / 60))::numeric, '$length_unit')\n\t\tELSE 1\n\tEND AS avg_speed_[[length_unit]]h,\n\t(max_battery_level - min_battery_level) AS battery_needed,\n\tconvert_km(distance_driven::decimal, '$length_unit') AS \"Distance driven\",\n\tconvert_km(((distance_driven::decimal * 100) / (max_battery_level - min_battery_level)), '$length_unit') AS max_distance\nFROM fnd\nWHERE \n\tCASE WHEN (duration_min / 60) > 0\n\t\tTHEN convert_km((distance_driven / (duration_min::numeric / 60))::numeric, '$length_unit')\n\t\tELSE 1\n\tEND > $min_avg_speed\n\tAND (max_battery_level - min_battery_level) > 0\nORDER BY distance_driven DESC\nLIMIT 17",
